# w3mimgdisplay
# move
keymap j MOVE_DOWN1
keymap k MOVE_UP1
keymap h MOVE_LEFT1
keymap l MOVE_RIGHT1
keymap C-e UP
keymap C-y DOWN
keymap C-f NEXT_PAGE
keymap C-b PREV_PAGE
keymap /   ISEARCH
keymap ?   ISEARCH_BACK
keymap t   SUBMIT

# buffer
keymap d CLOSE_TAB
keymap K PREV
keymap J NEXT
keymap s SELECT

# mark
keymap m   MARK
keymap C-n NEXT_MARK
keymap C-p PREV_MARK

# tab
keymap [ PREV_TAB
keymap ] NEXT_TAB
keymap o COMMAND  "GOTO file:///cgi-bin/toppage.cgi"
keymap O COMMAND  "TAB_GOTO file:///cgi-bin/toppage.cgi"
keymap ESC-e COMMAND  "GOTO file:///cgi-bin/searchword.cgi"
# keymap ESC-t "PIPE_BUF sed -e 's/\ /+/g'|xargs -I{} echo url?text={}&source=en&target=ja"
# あまりにも長いとそもそもあっちが耐えられない
# keymap ESC-t PIPE_BUF "sed -z 's/\n/+/g'|sed -e 's/\ /+/g'|sed -e 's/+\+/+/g'|sed -e 's/+$//'|xargs -I{} curl -G -L -s 'https://script.google.com/macros/s/AKfycbxrz3bxQ4qYVSiQ0pJheTJY48RMPYfAXjqkx6DzCX_cDKHA652eM31XvIzrfjTELY689Q/exec?text={}&source=en&target=ja'"

# outer command
keymap I EXEC_SHELL "export DISPLAY=$(echo $SSH_CLIENT| cut -d ' ' -f 1):0&& feh -g 1000x700 -. $W3M_CURRENT_IMG &"

# preference
keymap p OPTIONS

#shell
keymap ":"-! SHELL
keymap ":"-t SHELL zsh

# off
keymap Q NOTHING
keymap ":"-q ABORT
