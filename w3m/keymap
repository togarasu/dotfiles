# w3mimgdisplay
# move
keymap j MOVE_DOWN1
keymap k MOVE_UP1
keymap h MOVE_LEFT1
keymap l MOVE_RIGHT1
keymap C-e UP
keymap C-y DOWN
keymap C-f NEXT_PAGE
keymap C-b PREV_PAGE
keymap /   ISEARCH
keymap ?   ISEARCH_BACK
keymap C-RET   SUBMIT
keymap g-g BEGIN
keymap g-e END
keymap g-h LINE_BEGIN
keymap g-l LINE_END
keymap g-n NEXT
keymap g-p PREV

# buffer
keymap d CLOSE_TAB
# keymap K PREV
# keymap J NEXT
keymap s SELECT
keymap R COMMAND  "GOTO file:///cgi-bin/duplicate.cgi"
keymap r RELOAD

#link
keymap ":"-":" MARK_URL

# mark
keymap m   MARK
keymap [-m PREV_MARK
keymap ]-m NEXT_MARK
keymap t   GOTO file:///cgi-bin/quickmark.cgi

# tab
keymap [-t PREV_TAB
keymap ]-t NEXT_TAB
keymap o COMMAND  "DICT_WORD"
keymap O COMMAND  "TAB_GOTO file:///cgi-bin/toppage.cgi"
keymap ESC-e COMMAND  "GOTO file:///cgi-bin/searchword.cgi"

# edit screen
keymap ":"-e EDIT_SCREEN

# outer command

# preference
keymap p OPTIONS

#shell
keymap ":"-! SHELL
keymap ":"-t SHELL $SHELL

# off
keymap Q NOTHING
keymap ":"-q ABORT
